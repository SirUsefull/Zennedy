local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()



local Window = Rayfield:CreateWindow({
    Name = "Arm Wrestling Simulator",
    LoadingTitle = "loading...",
    LoadingSubtitle = "by Lui o lghd#0001",
 })


 local GetTab = Window:CreateTab("Stats Giver") 
 local FarmTab = Window:CreateTab("Auto Farm") 
 local TpTab = Window:CreateTab("Teleports") 
 local CreditsTab = Window:CreateTab("Credits") 




 local StatsGiverSection = GetTab:CreateSection("Stats Giver")

 local BicepInput = GetTab:CreateInput({
    Name = "Biceps Giver",
    PlaceholderText = "Max 100000000 (10b)",
    RemoveTextAfterFocusLost = true,
    Callback = function(Text)
        local args = {
            [1] = Text
        }
        
        workspace.Main3.RemoteEvent:FireServer(unpack(args))
    end,
 })


 local ForarmInput = GetTab:CreateInput({
    Name = "Forarm Giver",
    PlaceholderText = "Max 100000000 (10b)",
    RemoveTextAfterFocusLost = true,
    Callback = function(Text)
        local args = {
            [1] = Text
        }
        
        workspace.Main.RemoteEvent:FireServer(unpack(args))
    end,
 })

 local GripInput = GetTab:CreateInput({
    Name = "Grip Giver",
    PlaceholderText = "Max 100000000 (10b)",
    RemoveTextAfterFocusLost = true,
    Callback = function(Text)
        local args = {
            [1] = Text
        }
        
        workspace.Main2.RemoteEvent:FireServer(unpack(args))
    end,
 })

 local Label = GetTab:CreateLabel("If you got youself 10b then you can get up higher with the numbers")
    local Label = GetTab:CreateLabel("Have fun Saitama")

    local FarmSec = FarmTab:CreateSection("Auto Farm")

    local FarmBicepButton = FarmTab:CreateButton({
        Name = "Farm Biceps",
        Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
        Callback = function(Value)
        

                while wait(1) do
                    local args = {
                        [1] = 10000000000
                    }
                    
                    workspace.Main3.RemoteEvent:FireServer(unpack(args))
                end

        
        end,
     })

     local FarmGripButton = FarmTab:CreateButton({
        Name = "Farm Grip",
        Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
        Callback = function(Value)
        

                while wait(1) do
                    local args = {
                        [1] = 10000000000
                    }
                    
                    workspace.Main2.RemoteEvent:FireServer(unpack(args))
                end

         
        end,
     })


     local FarmForarmButton = FarmTab:CreateButton({
        Name = "Farm Forarm",
        Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
        Callback = function(Value)
        

                while wait(1) do
                    local args = {
                        [1] = 10000000000
                    }
                    
                    workspace.Main.RemoteEvent:FireServer(unpack(args))
                end

       
        end,
     })
 local TpSection = TpTab:CreateSection("Teleports")

 local TpSpawnButton = TpTab:CreateButton({
    Name = "Spawn",
    Callback = function()
        tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(2, Enum.EasingStyle.Linear)
        tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(38.00695037841797, 3.444831609725952, -78.92425537109375)}):Play()
    end,
 })

 local TpheavenButton = TpTab:CreateButton({
    Name = "Heaven",
    Callback = function()
        tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(2, Enum.EasingStyle.Linear)
        tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(617.642333984375, 563.144287109375, -98.18571472167969)}):Play()
    end,
 })


 local tphellButton = TpTab:CreateButton({
    Name = "Hell",
    Callback = function()
        tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(2, Enum.EasingStyle.Linear)
        tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(181.2910614013672, 22.393447875976562, 643.1844482421875)}):Play()
    end,
 })



 local tpGalaxyButton = TpTab:CreateButton({
    Name = "Galaxy",
    Callback = function()
        tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(2, Enum.EasingStyle.Linear)
        tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(-708.037353515625, 3.1441125869750977, -102.1283950805664)}):Play()
    end,
 })

